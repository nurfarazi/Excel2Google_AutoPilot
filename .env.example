# Absolute or relative path to the Excel file that will be uploaded.
EXCEL_FILE_PATH=./data/source.xlsx

# Path to the Google service account JSON credentials file.
GOOGLE_SERVICE_ACCOUNT_FILE=./credentials/service-account.json

# Spreadsheet ID from the Google Sheets URL.
GOOGLE_SPREADSHEET_ID=1replaceWithYourSpreadsheetId

# Name of the worksheet/tab within the spreadsheet to update.
GOOGLE_WORKSHEET_NAME=Sheet1

# Optional: comma separated list of column headers to export (keeps order); leave blank to use Excel headers as-is.
GOOGLE_COLUMNS=
